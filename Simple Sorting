import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner scanner=new Scanner(System.in);
	int x=scanner.nextInt();
	int i=0;
	int[] arr=new int[x];
	while(x-->0){
	    arr[i++]=scanner.nextInt();
	}
	Arrays.sort(arr);
	for(int number:arr){
	        System.out.println(number);
	}

	}
}
